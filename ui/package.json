{
  "name": "potato-facts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-staging": "tsc && vite build --config vite.config.staging.ts",
    "build-prod": "tsc && vite build --config vite.config.prod.ts",
    "build-dist-for-docker": "rm -fr dist && npm run build-staging && npm run build-prod",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@mantine/core": "^5.6.0",
    "@mantine/form": "^5.6.0",
    "@mantine/hooks": "^5.6.0",
    "@mantine/notifications": "^5.6.0",
    "dayjs": "^1.11.6",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^18.2.0",
    "typescript-optional": "^3.0.0-alpha.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
